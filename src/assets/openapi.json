  "info": {
    "title": "Seshat API",
    "version": "1"
  },
  "tags": [
    {
      "name": "accounts",
      "description": "Login/logout operations"
    },
    {
      "name": "campaigns",
      "description": "Operations to display and create campaigns"
    },
    {
      "name": "users",
      "description": "Users administration and creation"
    },
    {
      "name": "analytics",
      "description": "Operations to display and compute analytics on campaigns"
    },
    {
      "name": "tasks",
      "description": "Operations to manage, interact with and display tasks"
    }
  ],
  "openapi": "3.0.2",
  "components": {
    "schemas": {
      "LoginCredentials": {
        "type": "object",
        "properties": {
          "password": {
            "type": "string"
          },
          "login": {
            "type": "string"
          }
        },
        "required": [
          "login",
          "password"
        ]
      },
      "ConnectionToken": {
        "type": "object",
        "properties": {
          "token": {
            "type": "string"
          }
        },
        "required": [
          "token"
        ]
      },
      "NotificationData": {
        "type": "object",
        "properties": {
          "time": {
            "type": "string",
            "format": "date-time"
          },
          "object_type": {
            "type": "string"
          },
          "object_id": {
            "type": "string"
          },
          "notif_type": {
            "type": "string"
          },
          "message": {
            "type": "string"
          }
        },
        "required": [
          "message",
          "notif_type",
          "object_id",
          "object_type",
          "time"
        ]
      },
      "NotificationDelete": {
        "type": "object",
        "properties": {
          "notif_id": {
            "type": "string"
          }
        },
        "required": [
          "notif_id"
        ]
      },
      "CorporaListing": {
        "type": "object",
        "properties": {
          "folders_corpora": {
            "type": "array",
            "items": {
              "type": "string"
            }
          },
          "csv_corpora": {
            "type": "array",
            "items": {
              "type": "string"
            }
          }
        }
      },
      "TierSpecifications": {
        "type": "object",
        "properties": {
          "validate_tier": {
            "type": "boolean"
          },
          "allow_empty": {
            "type": "boolean"
          },
          "parser_name": {
            "type": "string"
          },
          "name": {
            "type": "boolean"
          },
          "content_type": {
            "type": "string"
          },
          "categories": {
            "type": "array",
            "items": {
              "type": "string"
            }
          },
          "required": {
            "type": "boolean"
          }
        },
        "required": [
          "name",
          "required",
          "validate_tier"
        ]
      },
      "CampaignCreation": {
        "type": "object",
        "properties": {
          "checking_scheme": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/TierSpecifications"
            }
          },
          "description": {
            "type": "string"
          },
          "name": {
            "type": "string",
            "maxLength": 100
          },
          "data_folder": {
            "type": "string"
          },
          "enable_audio_dl": {
            "type": "boolean"
          },
          "data_csv": {
            "type": "string"
          },
          "check_textgrids": {
            "type": "boolean"
          }
        },
        "required": [
          "enable_audio_dl",
          "name"
        ]
      },
      "CampaignEdit": {
        "type": "object",
        "properties": {
          "slug": {
            "type": "string"
          },
          "description": {
            "type": "string"
          },
          "name": {
            "type": "string"
          }
        },
        "required": [
          "description",
          "name",
          "slug"
        ]
      },
      "CampaignDelete": {
        "type": "object",
        "properties": {
          "slug": {
            "type": "string"
          }
        },
        "required": [
          "slug"
        ]
      },
      "CampaignShort": {
        "type": "object",
        "properties": {
          "check_textgrids": {
            "type": "boolean"
          },
          "corpus_path": {
            "type": "string"
          },
          "name": {
            "type": "string"
          },
          "assigned_files": {
            "type": "integer",
            "format": "int32"
          },
          "completed_tasks": {
            "type": "integer",
            "format": "int32"
          },
          "total_tasks": {
            "type": "integer",
            "format": "int32"
          },
          "annotators": {
            "type": "array",
            "items": {
              "type": "string"
            }
          },
          "total_files": {
            "type": "integer",
            "format": "int32"
          },
          "tiers_number": {
            "type": "integer",
            "format": "int32"
          }
        },
        "required": [
          "assigned_files",
          "check_textgrids",
          "completed_tasks",
          "corpus_path",
          "name",
          "total_files",
          "total_tasks"
        ]
      },
      "TaskShort": {
        "type": "object",
        "properties": {
          "status": {
            "type": "string"
          },
          "filename": {
            "type": "string"
          },
          "annotators": {
            "type": "array",
            "items": {
              "type": "string"
            }
          },
          "task_type": {
            "type": "string"
          },
          "deadline": {
            "type": "string",
            "format": "date"
          },
          "assigner": {
            "type": "string"
          },
          "creation_time": {
            "type": "string",
            "format": "date-time"
          },
          "is_locked": {
            "type": "boolean"
          }
        },
        "required": [
          "assigner",
          "creation_time",
          "filename",
          "is_locked",
          "status",
          "task_type"
        ]
      },
      "CampaignFull": {
        "type": "object",
        "properties": {
          "check_textgrids": {
            "type": "boolean"
          },
          "corpus_path": {
            "type": "string"
          },
          "name": {
            "type": "string"
          },
          "assigned_files": {
            "type": "integer",
            "format": "int32"
          },
          "completed_tasks": {
            "type": "integer",
            "format": "int32"
          },
          "total_tasks": {
            "type": "integer",
            "format": "int32"
          },
          "annotators": {
            "type": "array",
            "items": {
              "type": "string"
            }
          },
          "total_files": {
            "type": "integer",
            "format": "int32"
          },
          "tasks": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/TaskShort"
            }
          },
          "tiers_number": {
            "type": "integer",
            "format": "int32"
          }
        },
        "required": [
          "assigned_files",
          "check_textgrids",
          "completed_tasks",
          "corpus_path",
          "name",
          "total_files",
          "total_tasks"
        ]
      },
      "CampaignWikiPage": {
        "type": "object",
        "properties": {
          "content": {
            "type": "string"
          }
        }
      },
      "CampaignSubscriptionUpdate": {
        "type": "object",
        "properties": {
          "slug": {
            "type": "string"
          },
          "subscription_status": {
            "type": "boolean"
          }
        },
        "required": [
          "slug",
          "subscription_status"
        ]
      },
      "AnnotatorCreation": {
        "type": "object",
        "properties": {
          "password": {
            "type": "string"
          },
          "username": {
            "type": "string"
          },
          "last_name": {
            "type": "string"
          },
          "email": {
            "type": "string",
            "format": "email"
          },
          "first_name": {
            "type": "string"
          }
        },
        "required": [
          "email",
          "first_name",
